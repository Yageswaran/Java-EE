<?xml version="1.0" encoding="UTF-8" ?>

 <!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
       "http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd">


<tiles-definitions>



	<definition name="${YOUR_DEFINITION_HERE}">
	</definition>

	<definition name="FirstLayout" path="/layout.jsp">
		<put name="header" value="/header.jsp" />
		<put name="body" value="/body.jsp" />
		<put name="footer" value="/footer.jsp" />
	</definition>
	
	<definition name="excel" extends="FirstLayout">
		<put name="header" value="/header.jsp" />
		<put name="body" value="/excel.jsp" />

	</definition>
	
	<definition name="list" extends="FirstLayout">
		<put name="header" value="/header.jsp" />
		<put name="body" value="/list.jsp" />

	</definition>

</tiles-definitions>
